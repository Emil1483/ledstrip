generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        String     @id
  ledstrips Ledstrip[] @relation("UserLedstrips")
}

model Ledstrip {
  id    String @id
  name  String
  users User[] @relation("UserLedstrips")
}
